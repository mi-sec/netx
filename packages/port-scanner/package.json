	},
	"homepage": "https://github.com/mi-sec/netx#readme",
	"bugs": {
		"url": "https://github.com/mi-sec/netx/issues"
	},
	"dependencies": {
		"@mi-sec/lightmap": "0.1.8",
		"@mi-sec/network-cidr": "latest"
	},
	"devDependencies": {
		"chai": "4.2.0",
		"eslint": "7.4.0",
		"gen-esm-wrapper": "1.0.6",
		"mocha": "8.0.1",
